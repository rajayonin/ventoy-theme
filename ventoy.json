{
    "control":[
        { "VTOY_DEFAULT_KBD_LAYOUT": "SPANISH" }
    ],
    "theme":{
        "file":[
            "/ventoy/theme/theme_1920x1080.txt",
            "/ventoy/theme/theme_2880x1920.txt"
        ],
        "default_file": 1,
        "resolution_fit": 1,
        "gfxmode": "max",
        "ventoy_color": "#8aafc1",
        "fonts":[
            "/ventoy/theme/AdwaitaSans-24.pf2",
            "/ventoy/theme/terminus-14.pf2",
        ]
    },
    "menu_alias":[
        {
            "image": "/netboot.xyz.iso",
            "alias": "netboot.xyz"
        },
        {
            "image": "/cachyos-desktop-linux-2*****.iso",
            "alias": "CachyOS"
        },
        {
            "image": "/Fedora-Workstation-Live-42-*.*.x86_64.iso",
            "alias": "Fedora Workstation 42"
        },
        {
            "image": "/ubuntu-24.04.*-desktop-amd64.iso",
            "alias": "Ubuntu Desktop 24.04 LTS"
        },
        {
            "image": "/tails-amd64-6.**.iso",
            "alias": "TailsOS 6"
        },
        {
            "image": "/gparted-live-*.*.*-*-amd64.iso",
            "alias": "GParted"
        },
        {
            "image": "/linuxmint-22.*-cinnamon-64bit.iso",
            "alias": "Linux Mint 22 (Cinnamon)"
        },
        {
            "image": "/EndeavourOS_Mercury-Neo-202*.**.**.iso",
            "alias": "EndevourOS Mercury Neo"
        },
        {
            "image": "/boot-repair-disk-64bit.iso",
            "alias": "GRUB Boot-Repair"
        },
        {
            "image": "/microwin11.iso",
            "alias": "MicroWin 11"
        },
        {
            "image": "/microwin10.iso",
            "alias": "MicroWin 10"
        },
        {
            "image": "/manjaro-xfce-2*.*.*-******-linux6**.iso",
            "alias": "Manjaro (XFCE)"
        },
        {
            "image": "/pop-os_22.04_amd64_intel_53.iso",
            "alias": "Pop!_OS 22.04 LTS"
        },
        {
            "image": "/shredos-20**.**_**_x86-64_*.**_20******_vanilla.iso",
            "alias": "ShredOS"
        }
    ],
    "menu_class":[
